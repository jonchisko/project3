GodotProjectLogger="*res://scenes/autoloads/godot_project_logger.tscn"
QuestManager="*res://scenes/autoloads/quest_manager.tscn"
KnowledgeDatabaseService="*res://scenes/managers/knowledge_database/knowledge_database_service.tscn"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/mode=3
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/size/mode.debug=0

[editor_plugins]

enabled=PackedStringArray("res://addons/got_open_ai/plugin.cfg")

[file_customization]

folder_colors={
"res://scenes/test/": "pink",
"res://scenes/ui/": "green"
}

