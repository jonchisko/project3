config_version=5

[application]

config/name="Project3"
run/main_scene="res://scenes/ui/menu/main_menu_scene.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameEvents="*res://scenes/autoloads/game_events.gd"
OpenAiApi="*res://scenes/autoloads/open_ai_api.tscn"
GameTypes="*res://scenes/autoloads/game_types.gd"
ResourceDictionary="*res://scenes/autoloads/resource_dictionary.tscn"
OpenAiConfiguration="*res://scenes/autoloads/open_ai_configuration.tscn"
